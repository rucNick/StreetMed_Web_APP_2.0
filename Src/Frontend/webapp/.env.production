# Production Configuration for Google Cloud Run
VITE_BASE_URL=https://streetmed-backend-900663028964.us-central1.run.app
VITE_SECURE_BASE_URL=https://streetmed-backend-900663028964.us-central1.run.app
VITE_BASE_URL_HTTP=https://streetmed-backend-900663028964.us-central1.run.app

# Authentication and TLS settings
# Cloud Run handles TLS, so we use HTTPS URLs but don't need client-side TLS handling
VITE_USE_AUTH=true
VITE_USE_LOCAL_AUTH=false
VITE_USE_TLS=false
VITE_ALLOW_SELF_SIGNED_CERT=false

# Environment flag
VITE_ENVIRONMENT=production

# Production specific settings
VITE_DEBUG_MODE=false
VITE_ENABLE_LOGGING=false
VITE_LOG_LEVEL=error

# CORS and security settings
VITE_WITH_CREDENTIALS=true

# Timeout settings (in milliseconds)
VITE_REQUEST_TIMEOUT=30000
